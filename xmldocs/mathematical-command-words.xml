<xml version="1.0" encoding="utf-8" standalone="yes">
<!--
    operandIdentifier:
        0 - left and right
        1 - left left
        2 - right right
        3 - symbol
        4 - open bracket
        5 - close bracket
        6 - operand separator
        7 - constant
    priority:
        0 - highest (brackets)
        ...
        3 - lowest (add, subtract)
-->
    <open-bracket symbol="(" priority="3" operandIdentifier="4">
        <word>open bracket</word>
    </open-bracket>
    <close-bracket symbol=")" priority="3" operandIdentifier="5">
        <word>close bracket</word>
    </close-bracket>
    <addition symbol="+" priority="0" operandIdentifier="0">
        <word>add</word>
        <word>plus</word>
        <word>and</word>
        <word>more than</word>
    </addition>
    <subtraction symbol="-" priority="0" operandIdentifier="0">
        <word>subtract</word>
        <word>minus</word>
        <word>less</word>
        <word append="inv">less than</word>
        <word>take</word>
    </subtraction>
    <multiplication symbol="*" priority="1" operandIdentifier="0">
        <word>times</word>
        <word>multiplied by</word>
        <word>lots of</word>
    </multiplication>
    <division symbol="/" priority="1" operandIdentifier="0">
        <word>divided by</word>
        <word>over</word>
        <word>by</word>
    </division>
    <squared symbol="^" priority="2" operandIdentifier="0" roperand="2">
        <word>square</word>
        <word>squared</word>
        <word>SQ</word>
    </squared>
    <squareroot symbol="rt" priority="2" operandIdentifier="0" loperand="2">
        <word>square root</word>
        <word>sqrt</word>
    </squareroot>
    <cuberoot symbol="rt" priority="2" operandIdentifier="0" loperand="3">
        <word>cube root</word>
        <word>cbrt</word>
    </cuberoot>
    <root symbol="rt" priority="2" operandIdentifier="0">
        <word>root</word>
        <word>root of</word>
        <word>rt</word>
    </root>
    <power symbol="^" priority="2" operandIdentifier="0">
        <word>to the power of</word>
        <word>raised to</word>
        <word>to the</word>
    </power>
    <log symbol="log" priority="2" operandIdentifier="2" roperand="10 ,_3_6">
        <word>log</word>
        <word>log of</word>
    </log>
    <log symbol="log" priority="2" operandIdentifier="2">
        <word>log base</word>
    </log>
    <ln symbol="log" roperand="e_6 ,_3_6" priority="2" operandIdentifier="2">
        <word>ln</word>
        <word>natural log</word>
        <word>ln of</word>
        <word>natural log of</word>
    </ln>
    <of symbol="," priority="3" operandIdentifier="6">
        <word>of</word>
    </of>
</xml>